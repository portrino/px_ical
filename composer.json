{
    "name": "portrino/px_ical",
    "description": "Provides classes to render ical via eluceo iCal lib (https://github.com/markuspoerschke/iCal)",
    "type": "typo3-cms-extension",
    "keywords": [
        "TYPO3",
        "CMS",
        "ical",
        "ics",
        "typo3",
        "portrino"
    ],
    "homepage": "http://typo3.org",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Andre Wuttig",
            "email": "wuttig@portrino.de",
            "homepage": "http://www.portrino.de",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "dev@portrino.de",
        "issues": "https://github.com/portrino/px_ical/issues",
        "source": "https://github.com/portrino/px_ical"
    },
    "require": {
        "typo3/cms": "^7.6",
        "eluceo/ical": "~0.11.3"
    },
    "require-dev": {
        "nimut/testing-framework": "^1.0",
        "phpunit/phpunit": "~4.8.0",
        "squizlabs/php_codesniffer": "~3.0.2",
        "phpstan/phpstan": "^0.8"
    },
    "autoload": {
        "psr-4": {
            "Portrino\\PxICal\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Portrino\\PxICal\\Tests\\": "Tests"
        }
    },
    "config": {
        "vendor-dir": "build/vendor",
        "bin-dir": "build/bin"
    },
    "replace": {
        "px_ical": "*"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": "build/web"
        }
    }
}
